#24 - log.exp
x1 =int(input()) 
y1 =int(input()) 
x2 =int(input()) 
y2 =int(input()) 
if x1 == x2 and not y1 == y2: 
    print('YES') 
elif y1 == y2 and not x1 == x2: 
    print('YES') 
else: 
    print('NO')

    # 21 - log.exp
    a1, b1, c1 = int(input()), int(input()), int(input()) 
c = max(a1, b1, c1) 
b = min(a1, b1, c1) 
a = sum([a1, b1, c1]) - min(a1, b1, c1) - max(a1, b1, c1) 
if c >= a + b: 
    print('impossible') 
elif c**2 == a**2 + b**2: 
    print('rectangular') 
elif c**2 < a**2 + b**2: 
    print('acute') 
elif c**2 > a**2 + b**2: 
    print('obtuse')

    # 22 log.exp
    n = int(input("Enter number below 100: ")) 
print('На лугу пасется ',end='' ) 
if (n>100): 
    print("Incorrect number") 
elif((n>10 and n<20) or (n%10 >= 5) or (n%10==0)): 
    print(n,"коров") 
elif(n%10==1 ): 
    print(n,"корова") 
elif(n%10 in range(2,5)): 
    print(n,"коровы")


