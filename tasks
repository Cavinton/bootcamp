y = 4
z = 8

if x == y and y == z and x == z:
    print('3')
elif x == y or y == z or x == z:
    print('2')
else :
    print('0')


# №8

x = int(input(675))
y = int(input(23))
if x % y == 0: 
    print('x делится на y') 
    d = x//y
    print(f'Частное: {d}') 
else : 
    print('x не делится на y') 
    a = x // y 
    print(f'Частное: {a}') 
    b = x % y 
    print(f'Остаток: {b}')


# №18
num = int(input())
if num > 12 :
    print("Такого месяца нет")
elif num <=12 and num == 12 or num == 1 or num == 2:
    print("зима")
elif num <= 12 and num == 3 or num == 4 or num ==5 :
    print("весна")
elif num <= 12 and num == 6 or num == 7 or num == 8 :
    print("лето")
elif num <= 12 and num == 9 or num == 10 or num == 11 :
    print("осень")


